{
  "name": "vite-plugin-preload-assets",
  "version": "1.2.0",
  "description": "Optimizes performance by automatically injecting preload and preconnect tags for critical resources with Vite (JS, CSS, fonts, images).",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "preload",
    "preconnect",
    "html preload",
    "critical resources",
    "performance",
    "web performance",
    "loading optimization",
    "fonts preload",
    "js preload",
    "css preload",
    "vite html plugin",
    "vite performance plugin",
    "lazy loading",
    "critical js",
    "critical css",
    "image preload",
    "vite multi page"
  ],
  "author": "Effeilo",
  "license": "MIT",
  "repository": {
      "type": "git",
      "url": "https://github.com/Effeilo/vite-plugin-preload-assets"
  },
  "bugs": {
      "url": "https://github.com/Effeilo/vite-plugin-preload-assets/issues"
  },
  "homepage": "https://github.com/Effeilo/vite-plugin-preload-assets",
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0 || ^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.7",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  }
}
